{
    "name": "maggidev/api",
    "description": "API RESTful para scraping de animes, mangÃ¡s e hentais",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.8",
        "symfony/dom-crawler": "^6.4",
        "symfony/css-selector": "^6.4",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "AnimeScraper\\": "src/"
        }
    },
    "scripts": {
        "vercel": [
            "composer install --optimize-autoloader --no-dev --prefer-dist --no-progress --no-suggest --classmap-authoritative"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}