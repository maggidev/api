# Chave secreta para autenticação do app (gere uma chave aleatória forte)
APP_SECRET_KEY=brdoxsecretkey

# Chave de administrador para acesso aos logs
ADMIN_LOG_KEY=brdoxsecretkey

# Rate limiting (requisições por hora por IP)
RATE_LIMIT_PER_HOUR=100

# Cache TTL em segundos (1 hora = 3600)
CACHE_TTL=3600

# Logs - dias para manter logs antigos
LOG_RETENTION_DAYS=7

# Ambiente (development ou production)
APP_ENV=production

# CORS - origens permitidas (separadas por vírgula)
CORS_ORIGINS=*
